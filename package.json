{
  "name": "@trylonai/ts-result",
  "version": "1.0.0",
  "description": "Rust's Result type for type-safe error handling in TypeScript.",
  "main": "./dist/cjs/index.cjs",
  "author": "Kerim Buyukakyuz <kerim@trylon.ai>",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "docs:generate": "typedoc"
  },
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "result",
    "rust",
    "error handling",
    "functional programming",
    "typescript",
    "trylon",
    "type-safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/trylonai/ts-result.git"
  },
  "bugs": {
    "url": "https://github.com/trylonai/ts-result/issues"
  },
  "homepage": "https://github.com/trylonai/ts-result#readme",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vitest": "^3.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
